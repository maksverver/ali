verb QUIT, EXIT.

entity CHEST, BOX.

function test($arg, $foo, $bar)
{
   if ($arg) {
       return $foo;
   } else {
       return $bar;
   }
}

function get($e)
{
    return not [$e].value;
}

function initialize()
{
    set [CHEST].value true;
    set @bar test(true, "World", "Monde");
    set $foo test(get(BOX), "Bonjour", "Hello");
    write($foo, @bar);
}

command QUIT {
    quit()
}
